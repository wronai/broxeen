# ── Broxeen LLM Configuration ────────────────────────
# Copy to .env and fill in your API key
#
# Get your key at: https://openrouter.ai/keys

# Frontend (Vite) — available in browser JS (dev mode only!)
VITE_OPENROUTER_API_KEY=sk-or-v1-your-key-here
VITE_LLM_MODEL=google/gemini-3-flash-preview
VITE_LLM_MAX_TOKENS=2048
VITE_LLM_TEMPERATURE=0.7

# Backend (Tauri/Rust) — secure, not exposed to frontend
OPENROUTER_API_KEY=sk-or-v1-your-key-here
LLM_MODEL=google/gemini-3-flash-preview
LLM_MAX_TOKENS=2048
LLM_TEMPERATURE=0.7
