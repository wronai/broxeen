{
  "name": "broxeen",
  "private": true,
  "version": "1.0.80",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:browser": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ssh": "playwright test e2e/ssh-docker.spec.ts",
    "ssh:setup": "cd docker/ssh-test && chmod +x setup.sh && ./setup.sh",
    "ssh:start": "cd docker/ssh-test && docker-compose up -d",
    "ssh:stop": "cd docker/ssh-test && docker-compose down",
    "ssh:logs": "cd docker/ssh-test && docker-compose logs -f",
    "health:check": "node -e \"import('./src/utils/healthCheck.js').then(m => m.runHealthCheck().then(console.log))\"",
    "health:quick": "node -e \"import('./src/utils/healthCheck.js').then(m => m.runQuickHealthCheck().then(console.log))\"",
    "diagnose": "npm run health:check",
    "cli": "node scripts/chat-cli.mjs",
    "net-diag": "node scripts/net-diag.mjs",
    "net-diag:cameras": "node scripts/net-diag.mjs --cameras",
    "net-diag:compare": "node scripts/net-diag.mjs --compare",
    "lint:ts": "tsc --noEmit",
    "lint:rust": "cargo fmt --manifest-path src-tauri/Cargo.toml -- --check && cargo clippy --manifest-path src-tauri/Cargo.toml -- -D warnings",
    "lint:py": "python -m black --check phonetic.py test_phonetic.py",
    "lint": "npm run lint:ts && npm run lint:rust && npm run lint:py",
    "check": "npm run lint:ts"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "better-sqlite3": "^12.6.2",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tauri-apps/cli": "^2.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.0.0",
    "autoprefixer": "^10.4.20",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  },
  "author": "Tom Sapletta <tom@sapletta.com>",
  "license": "Apache-2.0"
}
